{
  "name": "chessboardjsx",
  "version": "1.0.1",
  "description": "chessboardjsx is a standalone chessboard built with React and inspired by chessboardjs",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "cross-env NODE_ENV=test npx jest --coverage",
    "prebuild": "rimraf dist ",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "lint": "eslint src",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel test lint build",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/willb335/chessboardjsx.git"
  },
  "author": "William .J Bashelor <wbashelor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willb335/chessboardjsx/issues"
  },
  "homepage": "https://github.com/willb335/chessboardjsx#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "6.2.0",
    "@commitlint/config-conventional": "6.1.3",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.3",
    "babel-preset-react-app": "3.1.1",
    "babel-register": "6.26.0",
    "codecov": "3.0.2",
    "commitizen": "2.9.6",
    "cross-env": "5.1.5",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.8.2",
    "ghooks": "2.0.4",
    "jest": "22.4.3",
    "npm-run-all": "4.1.3",
    "prettier": "1.12.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "rimraf": "2.6.2",
    "semantic-release": "^15.4.1",
    "travis-deploy-once": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "npm run commitmsg"
    }
  }
}
